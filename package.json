{
  "name": "oaf-react-final-form",
  "version": "0.22.0",
  "main": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oaf-project/oaf-react-final-form.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "final-form": "^4.18.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "prettylint": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "tslint-react": "^4.0.0",
    "tslint-react-a11y": "^1.0.0",
    "tslint-react-hooks": "^2.2.1",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "type-coverage": "type-coverage --strict --detail",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "format-lint": "prettylint '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "test": "jest",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn format-lint && yarn publish"
  },
  "peerDependencies": {
    "react-final-form": "^6.3.0"
  },
  "dependencies": {
    "fp-ts": "^2.0.2",
    "io-ts": "^2.0.0",
    "io-ts-types": "^0.5.0",
    "monocle-ts": "^2.0.0",
    "oaf-side-effects": "^2.0.0"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
