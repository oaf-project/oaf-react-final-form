{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-immutable/all",
        "tslint-sonarts",
        "tslint-react",
        "tslint-react-a11y",
        "tslint-react-hooks",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "no-unsafe-any": true,
        "interface-over-type-literal": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "react-hooks-nesting": true
    },
    "rulesDirectory": []
}